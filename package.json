{
  "name": "change-to-language-mode",
  "displayName": "Change To Language Mode",
  "description": "Provides JUST one command to Change Language Mode.",
  "version": "0.0.5",
  "publisher": "NferZhuang",
  "license": "MIT",
  "engines": {
    "vscode": "^1.50.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nfer/vscode-change-to-language-mode"
  },
  "bugs": {
    "url": "https://github.com/nfer/vscode-change-to-language-mode/issues"
  },
  "keywords": [
    "change",
    "language",
    "mode"
  ],
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:change-to-language-mode.changeToHTML",
    "onCommand:change-to-language-mode.changeToHTMLAndFormat",
    "onCommand:change-to-language-mode.changeToJavaScript",
    "onCommand:change-to-language-mode.changeToJavaScriptAndFormat",
    "onCommand:change-to-language-mode.changeToCSS",
    "onCommand:change-to-language-mode.changeToCSSAndFormat",
    "onCommand:change-to-language-mode.changeToJSON",
    "onCommand:change-to-language-mode.changeToJSONAndFormat",
    "onCommand:change-to-language-mode.changeToTypeScript",
    "onCommand:change-to-language-mode.changeToTypeScripAndFormat",
    "onCommand:change-to-language-mode.changeToMarkdown",
    "onCommand:change-to-language-mode.changeToPlainText"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "change-to-language-mode.changeToHTML",
        "title": "Change to HTML Mode"
      },
      {
        "command": "change-to-language-mode.changeToHTMLAndFormat",
        "title": "Change to HTML Mode And Format"
      },
      {
        "command": "change-to-language-mode.changeToJavaScript",
        "title": "Change to JavaScript Mode"
      },
      {
        "command": "change-to-language-mode.changeToJavaScriptAndFormat",
        "title": "Change to JavaScript Mode And Format"
      },
      {
        "command": "change-to-language-mode.changeToCSS",
        "title": "Change to CSS Mode"
      },
      {
        "command": "change-to-language-mode.changeToCSSAndFormat",
        "title": "Change to CSS Mode And Format"
      },
      {
        "command": "change-to-language-mode.changeToJSON",
        "title": "Change to JSON Mode"
      },
      {
        "command": "change-to-language-mode.changeToJSONAndFormat",
        "title": "Change to JSON Mode And Format"
      },
      {
        "command": "change-to-language-mode.changeToTypeScript",
        "title": "Change to TypeScript Mode"
      },
      {
        "command": "change-to-language-mode.changeToTypeScriptAndFormat",
        "title": "Change to TypeScript Mode And Format"
      },
      {
        "command": "change-to-language-mode.changeToMarkdown",
        "title": "Change to Markdown Mode"
      },
      {
        "command": "change-to-language-mode.changeToPlainText",
        "title": "Change to PlainText Mode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
